generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Attempt {
  id Int @id @default(autoincrement())
  frequency Frequency[]
  createdAt DateTime @default(now())
}

model Frequency {
  id Int @id @default(autoincrement())
  id_attempt Int
  attempt Attempt @relation(fields: [id_attempt], references: [id])
  frequency String
  rssi RSSI[]
}

model RSSI {
  id Int @id @default(autoincrement())
  id_frequency Int
  frequency Frequency @relation(fields: [id_frequency], references: [id])
  rssi Int
}